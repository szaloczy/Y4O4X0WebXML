<!DOCTYPE mozi [
    <!ELEMENT mozi (film*)>
    <!ELEMENT film (rendezo+, szinesz+, iro+)>
    <!ELEMENT rendezo (#PCDATA)>
    <!ELEMENT szinesz (#PCDATA)>
    <!ELEMENT iro (#PCDATA)>
 
    <!ATTLIST film id ID #REQUIRED>
    <!ATTLIST film mufaj (akció | horror | vígjáték) #REQUIRED>
    <!ATTLIST rendezo id ID #REQUIRED>
    <!ATTLIST rendezo nev CDATA #REQUIRED>
    <!ATTLIST szinesz id ID #REQUIRED nev CDATA #REQUIRED>
    <!ATTLIST szinesz nev CDATA #REQUIRED>
    <!ATTLIST iro id ID #REQUIRED>  
]>

<mozi>
    <film id="txn_nxm_2jb" mufaj="horror">
        <rendezo id="nzk_4xm_2jb" nev="alex"></rendezo>
        <szinesz id="u5q_4xm_2jb" nev="Madison_Ivy"></szinesz>
        <iro id="trv_4xm_2jb"/>
    </film>
</mozi>
